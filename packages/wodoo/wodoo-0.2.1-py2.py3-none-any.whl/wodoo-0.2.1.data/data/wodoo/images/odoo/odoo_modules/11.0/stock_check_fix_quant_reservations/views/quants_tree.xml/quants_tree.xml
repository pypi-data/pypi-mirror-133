<?xml version="1.0"?>
<odoo>
  <data noupdate="0">
    <record id="quants_reservations_fix_tree" model="ir.ui.view">
      <field name="model">stock.quant</field>
      <field name="arch" type="xml">
        <tree default_order="" string="">
          <button name="fix_reservation" string="Fix Reservation" type="object" attrs="{'invisible': [('needs_fix_reservation', '=', False)]}"/>
          <field name="id" groups="base.group_system"/>
          <field name="product_id"/>
          <field name="needs_fix_reservation" invisible='1'/>
          <field name="calculated_reservations"/>
          <field name="lot_id" />
          <field name="quantity" />
          <field name="reserved_quantity"/>
          <field name="location_id" />
        </tree>
      </field>
    </record>
  </data>
</odoo>
