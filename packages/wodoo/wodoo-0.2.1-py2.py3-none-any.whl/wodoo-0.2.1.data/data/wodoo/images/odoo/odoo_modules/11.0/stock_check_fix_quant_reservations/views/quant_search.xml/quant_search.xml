<?xml version="1.0"?>
<odoo>
  <data noupdate="0">
    <record id="view_stock_quant_search1" model="ir.ui.view">
      <field name="model">stock.quant</field>
      <field name="inherit_id" ref="stock.quant_search_view"/>
      <field name="arch" type="xml">
        <filter name="reserved" position="after">
          <filter name="over_reservation" domain="[('reserved_quantity', '>', 0), ('location_id.usage', '=', 'internal'), ('over_reservation', '=', True)]" string="Problem: Too much reserved"/>
          <filter name="needs_fix_reservation" domain="[('needs_fix_reservation', '=', True)]" string="Needs Fix"/>
        </filter>
      </field>
    </record>
  </data>
</odoo>
