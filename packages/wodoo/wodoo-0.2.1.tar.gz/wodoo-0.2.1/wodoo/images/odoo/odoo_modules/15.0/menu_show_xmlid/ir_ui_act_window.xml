<?xml version="1.0"?>
<odoo>
  <data>
    <record id="view_ir_actions_act_window_form" model="ir.ui.view">
      <field name="name">add xmlid</field>
      <field name="model">ir.actions.act_window</field>
      <field name="inherit_id" ref="base.view_window_action_form"/>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <field name="target" position="after">
          <field name="xml_id"/>
        </field>
      </field>
    </record>
    <record id="view_ir_actions_act_window_tree" model="ir.ui.view">
      <field name="name">add xmlid</field>
      <field name="model">ir.actions.act_window</field>
      <field name="type">tree</field>
      <field name="inherit_id" ref="base.view_window_action_tree"/>
      <field name="arch" type="xml">
        <field name="domain" position="after">
          <field name="xml_id"/>
        </field>
      </field>
    </record>
  </data>
</odoo>
