<?xml version="1.0"?>
<odoo>
  <data noupdate="0">
    <record id="view_res_lang_form" model="ir.ui.view">
      <field name="model">res.lang</field>
      <field name="inherit_id" ref="base.res_lang_form"/>
      <field name="arch" type="xml">
        <xpath expr="//group/group[last()]" position="after">
          <group>
            <field name="excel_format_num"/>
            <field name="excel_format_money"/>
            <field name="excel_format_date"/>
          </group>
        </xpath>
      </field>
    </record>
  </data>
</odoo>
